<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Database</title>
     <!-- bootstrap 4 css -->
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css">
  <!-- google font -->
  <link href="https://fonts.googleapis.com/css?family=Public+Sans:300,500&display=swap" rel="stylesheet">
    <style>
        body {
  font: 300 14px/1.2 'Public Sans', sans-serif;
}

.container-fluid {
  max-width: 1024px;
}

#auth{
  opacity: 0.5;
}

.gap{
   height: 100px;
}
    </style>
</head>
<body>
    <div class="container-fluid mt-3">
        <h1>Events List</h1><br><br>
     <!-- responsive table -->
     <div class="table-responsive">
        <table class="table table-bordered">
          <thead>
            <tr class="table-info">
              <th scope="col">Event Name </th>
              <th scope="col">descrition</th>
             <th scope="col">Date</th>
             
            </tr>
          </thead>
          <tbody>
          <% for(var i=0;i<data.length;i++){ %>
            <tr>
                <td><a href="/<%=data[i].addname%>/data"><%=data[i].addname%></a></td>
                <td><%=data[i].des%></td>
                <td><%=data[i].date%></td>
                
              </tr> 
          <% }%>

         
              

              
          </tbody>
        </table>
        
      </div>
      
    </div>
    <center>
        <div class="gap">

        </div>
    <a href="/event" class="btn btn-outline-dark btn-lg my-5 mx-3" >Home</a>
</center>
</body>
</html>